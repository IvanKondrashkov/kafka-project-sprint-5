{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Order",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "name": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "amount": {
      "type": "number"
    },
    "quantity": {
      "type": "integer"
    },
    "dateCreated": {
      "type": "string",
      "format": "date-time",
      "pattern": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}$"
    }
  },
  "required": [
    "id",
    "name",
    "category",
    "amount",
    "quantity",
    "dateCreated"
  ]
}